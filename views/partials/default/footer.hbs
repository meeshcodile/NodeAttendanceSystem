<script>
  const date = new Date()
  const hours = date.getHours()
  const myButton = document.getElementById('btnControl1')
  const myButton2 = document.getElementById('btnControl2')


  if (hours >= 8 && hours <= 10) myButton.disabled = false, myButton.style.cursor = 'pointer'
  else myButton.disabled = true, myButton.innerHTML = 'Button Unavailable'


  if (hours >= 12 && hours <= 18) myButton2.disabled = false, myButton2.style.cursor = 'pointer'
  else myButton2.disabled = true, myButton2.innerHTML = 'Button Unavailable'
</script>

